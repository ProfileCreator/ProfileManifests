			<key>pfm_description_reference</key>
			<string>https://github.com/SAP/macOS-enterprise-privileges/tree/master/application_management</string>
			<key>pfm_name</key>
			<string>DockToggleMaxTimeout</string>
			<key>pfm_note</key>
